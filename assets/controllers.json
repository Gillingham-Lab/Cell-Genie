{
    "controllers": {
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": false,
                "fetch": "eager"
            }
        },
        "@tienvx/ux-collection-js": {
            "collection": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "@tienvx/ux-collection-js/src/style.css": true
                }
            }
        }
    },
    "entrypoints": []
}
