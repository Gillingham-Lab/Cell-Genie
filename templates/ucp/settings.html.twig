{% extends 'homepage.html.twig' %}

{% block title %}Gin :: {{ title }}{% endblock %}

{% block body_main %}
<div class="row">
    <div class="container container-fluid w-100 p-0 m-0">
        <twig:PageHeader :title="title" icon="{{ icon }}" :iconStack="iconStack" />

        <twig:Layout:Row>
            <twig:Layout:Col type="inner">
                {# <twig:Toolbox :toolbox="toolbox" />  #}
            </twig:Layout:Col>
        </twig:Layout:Row>


        <twig:Layout:Row>
            <twig:Layout:Col type="outer" class="col-12">
                <twig:ElementCard title="Settings" icon="settings">
                    <twig:Form:TabbedFormComponent form="{{ form }}" saveButtonLabel=""/>
                </twig:ElementCard>
            </twig:Layout:Col>
        </twig:Layout:Row>
    </div>
</div>
{% endblock %}