<nav{{ attributes.defaults({"class": "navbar navbar-expand-lg navbar-dark bg-dark"})}} id="{{ id }}">
    <div class="container-fluid">
        {% block navbrand %}{% endblock %}

        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#{{ this.containerId }}" aria-controls="{{ this.containerId }}" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="{{ this.containerId }}">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                {% block content %}{% endblock %}
            </ul>

            {% block navsearch %}{% endblock %}
        </div>
    </div>
</nav>
