{
    "name": "gillingham-lab/cell-genie",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "description": "A php-based web tool for small chemical biology labs.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-pdo_pgsql": "*",
        "ext-sqlite3": "*",
        "ext-zip": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "dev-DDC-2917-2.23 as 2.13.1",
        "easycorp/easyadmin-bundle": "^4.0",
        "friendsofsymfony/ckeditor-bundle": "^2.4",
        "gedmo/doctrine-extensions": "^3.7",
        "google/apiclient": "^2.12.1",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "*",
        "symfony/form": "^6.1",
        "symfony/framework-bundle": "*",
        "symfony/html-sanitizer": "^6.1",
        "symfony/monolog-bundle": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/uid": "*",
        "symfony/ux-turbo": "^2.10",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "^1.15",
        "symfony/yaml": "*",
        "tecnickcom/tcpdf": "^6.5",
        "tienvx/ux-collection-js": "1.1",
        "twig/extra-bundle": "^3.6",
        "twig/string-extra": "^3.6"
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/Gillingham-Lab/doctrine-orm-fork"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "pre-autoload-dump": [
            "Google\\Task\\Composer::cleanup"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "google/apiclient-services": [
            "Calendar"
        ],
        "symfony": {
            "allow-contrib": false,
            "require": "^6.1"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "symfony/maker-bundle": "^1.30",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "*",
        "symfony/twig-bundle": "*",
        "symfony/web-profiler-bundle": "*"
    }
}
